    "aws-jwt-verify": "^2.1.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "http-auth": "^4.2.0",
    "http-auth-passport": "^1.0.7",
